# Discord Bot Configuration
DISCORD_TOKEN=your_bot_token_here
CLIENT_ID=your_client_id_here

# Environment
NODE_ENV=production

# Default Pride Events Channel (can be overridden per server)
DEFAULT_PRIDE_CHANNEL_ID=your_default_channel_id

# Timezone (optional, defaults to UTC)
TZ=UTC

# Logging level (optional)
LOG_LEVEL=info