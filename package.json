{
  "name": "clove-bot",
  "version": "1.0.0",
  "description": "A Discord Bot made by CloveTwilight3",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "dev:watch": "ts-node --watch src/index.ts",
    "watch": "tsc -w",
    "deploy-commands": "ts-node src/deploy-commands.ts",
    "deploy-commands:prod": "npm run build && node dist/deploy-commands.js",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "clean": "rm -rf dist"
  },
  "keywords": ["discord", "bot", "typescript"],
  "author": "CloveTwilight3",
  "license": "MIT",
  "dependencies": {
    "discord.js": "^14.15.3",
    "dotenv": "^16.4.5",
    "rss-parser": "^3.13.0",
    "@atproto/api": "^0.6.23",
    "node-cron": "^3.0.3",
    "cheerio": "^1.0.0-rc.12",
    "axios": "^1.6.0"
  },
  "devDependencies": {
    "@types/node": "^20.14.2",
    "@typescript-eslint/eslint-plugin": "^7.12.0",
    "@typescript-eslint/parser": "^7.12.0",
    "eslint": "^8.57.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5",
    "@types/node-cron": "^3.0.11",
    "@types/cheerio": "^0.22.35"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}